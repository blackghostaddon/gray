def GetLSProData(page_data,Cookie_Jar,m,):
  import time, requests, re, HTMLParser
  from bisect import bisect_left
  time = time.strftime("%Y%m%d%H%M%S", time.gmtime())
  try:
   epg = 'http://media.entutele.com/epg/feed.xml'
   source = requests.get(epg).text
#   ch = 'TF1'
   ch = '[makelist.param1]'
#   ch = 'CANAL\+'
#   ch = 'France 3 Corse Via Stella'
#   ch = 'Franceinfo'
   regex = 'start="(\d+?) .+?channel="' + ch
   start = re.findall(regex, source)
   pos = (bisect_left(start, time))
   t1 = start[pos]
   t2 = start[pos + 1]
   regex = 'start="' + t1 + '.+?channel="' + ch + '">[\\n\\r]*.+?title lang="es">(.+?)<'
   now = re.findall(regex, source)[0]
   regex = 'start="' + t2 + '.+?channel="' + ch + '">[\\n\\r]*.+?title lang="es">(.+?)<'
   next = re.findall(regex, source)[0]
  except:
   return ' No program data'
  return ' [B]Ahora: ' + HTMLParser.HTMLParser().unescape(now) + '\n[/B] Despues: ' + HTMLParser.HTMLParser().unescape(next)
]]></expres>
